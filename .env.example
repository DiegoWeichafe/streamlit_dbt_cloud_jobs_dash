# dbt Cloud API Configuration
# Copy this file to .env and fill in your actual values

# Your dbt Cloud API token (get this from your dbt Cloud account settings)
DBT_CLOUD_API_TOKEN=your_api_token_here

# Your dbt Cloud account ID (must be a string containing only numbers)
DBT_CLOUD_ACCOUNT_ID=your_account_id_here

DBT_CLOUD_BASE_URL=https://xyz.dbt.com
